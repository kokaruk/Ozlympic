        </GridPane>
        <Button layoutX="14.0" layoutY="165.0" minWidth="70.0" mnemonicParsing="false" onAction="#handleOk" text="OK"
                AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="5.0"/>
