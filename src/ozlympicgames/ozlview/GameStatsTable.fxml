<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minWidth="250.0" xmlns="http://javafx.com/javafx/8.0.111">
    <TableView fx:id="gamesTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="gameIdColumn" maxWidth="70.0" minWidth="70.0" resizable="false" text="Game ID"/>
            <TableColumn fx:id="gameSportColumn" minWidth="100.0" prefWidth="100.0" resizable="false" text="Sport"/>
            <TableColumn fx:id="timeRunColumn" minWidth="130.0" prefWidth="150.0" text="Date Run"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
