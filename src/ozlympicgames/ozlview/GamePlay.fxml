<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="ozlympicgames.ozlcontroller.GamePlayController">
    <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
                <children>

                    <VBox prefHeight="175.0" prefWidth="100.0" AnchorPane.bottomAnchor="150.0"
                          AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="20.0">
                        <children>
                            <Label fx:id="activeGameName" text="Label">
                                <font>
                                    <Font name="System Bold" size="72.0"/>
                                </font>
                            </Label>
                            <Label fx:id="activeRefereeName" prefHeight="39.0" prefWidth="559.0" text="Label">
                                <font>
                                    <Font size="32.0"/>
                                </font>
                            </Label>
                            <Label fx:id="champ1" text="Label">
                                <VBox.margin>
                                    <Insets top="10.0"/>
                                </VBox.margin>
                            </Label>
                            <Label fx:id="champ2" text="Label">
                                <VBox.margin>
                                    <Insets top="10.0"/>
                                </VBox.margin>
                            </Label>
                            <Label fx:id="champ3" text="Label">
                                <VBox.margin>
                                    <Insets top="10.0"/>
                                </VBox.margin>
                            </Label>
                        </children>
                    </VBox>

                    <Button layoutX="15.0" layoutY="346.0" minWidth="70.0" mnemonicParsing="false" onAction="#handleOk"
                            text="OK" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"/>
                    <Button layoutX="123.0" layoutY="359.0" minWidth="70.0" mnemonicParsing="false"
                            onAction="#handleCancel" text="Cancel" AnchorPane.bottomAnchor="10.0"/>
                    <Button fx:id="startButton" layoutX="455.0" layoutY="280.0" mnemonicParsing="false"
                            onAction="#handleGameStart" prefHeight="106.0" prefWidth="131.0" text="Start"
                            AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
                </children>
            </AnchorPane>
        </children>
    </StackPane>

</AnchorPane>
