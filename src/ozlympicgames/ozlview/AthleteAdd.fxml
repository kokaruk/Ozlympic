<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="ozlympicgames.ozlcontroller.AthleteAddController">
    <children>
        <GridPane layoutX="84.0" layoutY="57.0" prefHeight="165.0" prefWidth="397.0" AnchorPane.leftAnchor="5.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Name:"/>
                <Label text="State:" GridPane.rowIndex="1"/>
                <Label text="Age:" GridPane.rowIndex="2"/>
                <Label text="SportType:" GridPane.rowIndex="3"/>
                <Label fx:id="athlName" text="Label" GridPane.columnIndex="1"/>
                <Label fx:id="athlState" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label fx:id="athlAge" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <ComboBox fx:id="athlType" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
            </children>
        </GridPane>
        <Button layoutX="14.0" layoutY="165.0" minWidth="70.0" mnemonicParsing="false" onAction="#handleOk" text="OK"/>
        <Button layoutX="93.0" layoutY="165.0" minWidth="70.0" mnemonicParsing="false" onAction="#handleCancel"
                text="Cancel"/>
    </children>
</AnchorPane>
